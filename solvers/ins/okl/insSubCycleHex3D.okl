          s_cubInterpT[0][id] = cubInterpT[id];
        }
	
        s_cubD[0][id] = cubD[id];
	
	for(int k=0;k<p_cubNq;++k){
	  const dlong id = e*p_cubNp*3 + k*p_cubNq*p_cubNq + j*p_cubNq + i;	  	  
	  
