	    const dfloat Wc = U[id+2*offset];

	    for(int k=0;k<p_cubNq;++k){
	      const dfloat Ikc = s_cubInterpT[c][k];
	      
	      r_U[k] += Ikc*Uc;
	      r_V[k] += Ikc*Vc;
	      r_W[k] += Ikc*Wc;
	    }
	  }	  
	}
      }
    }

