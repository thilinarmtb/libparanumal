	  // I_f^t*(J_f*C_f^t)*G_f*\hat{D}_f*I_f*u
	  r_U[k] = Uhat*Udr + Vhat*Uds + What*Udt;
	  r_V[k] = Uhat*Vdr + Vhat*Vds + What*Vdt;
	  r_W[k] = Uhat*Wdr + Vhat*Wds + What*Wdt;
	}
      }
    }
    
