	  if(b<p_Nq){
	    dfloat rhsU = 0, rhsV = 0, rhsW = 0;

	    for(int j=0;j<p_cubNq;++j){
	      dfloat Ijb = s_cubInterpT[b][j];
	      rhsU += Ijb*s_U[j][i];
	      rhsV += Ijb*s_V[j][i];
	      rhsW += Ijb*s_W[j][i];
	    }

	    s_Ud[b][i] = rhsU;
	    s_Vd[b][i] = rhsV;
	    s_Wd[b][i] = rhsW;
	  }
	}
      }
      
      @barrier("local");
      
      for(int b=0;b<p_cubNq;++b;@inner(1)){
	for(int a=0;a<p_cubNq;++a;@inner(0)){
	  if(a<p_Nq && b<p_Nq){
	    dfloat rhsU = 0, rhsV = 0, rhsW = 0;

	    for(int i=0;i<p_cubNq;++i){
	      dfloat Iia = s_cubInterpT[a][i];
	      rhsU += Iia*s_Ud[b][i];
	      rhsV += Iia*s_Vd[b][i];
	      rhsW += Iia*s_Wd[b][i];
	    }

            const dlong id = e*p_Np + c*p_Nq*p_Nq + b*p_Nq + a;
	    dfloat invMM = invLumpedMassMatrix[id];
            NU[id+0*offset] = invMM*rhsU; // need to assemble then divide by mass after kernel
            NU[id+1*offset] = invMM*rhsV;
            NU[id+2*offset] = invMM*rhsW;
          }
        }
      }
    }
  }
