	    Udr += Din*s_Ud[j][n];
	    Vdr += Din*s_Vd[j][n];
	    Wdr += Din*s_Wd[j][n];
	  }

	  for(int n=0;n<p_cubNq;++n){
	    dfloat Djn = s_cubD[j][n];
	    Uds += Djn*s_Ud[n][i];
	    Vds += Djn*s_Vd[n][i];
	    Wds += Djn*s_Wd[n][i];
	  }
	  
	  for(int n=0;n<p_cubNq;++n){
